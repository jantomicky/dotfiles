{
    "update.mode": "none",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "workbench.colorTheme": "Ayu Adapta",
    "workbench.colorCustomizations": {
        "[Ayu Adapta]": {
            "list.focusForeground": "#FFFFFF",
            "list.focusBackground": "#303B44",
            "list.highlightForeground": "#5CCFE6",
            "list.activeSelectionBackground": "#3D4752",
            "list.activeSelectionForeground": "#FFFFFF",
            "titleBar.activeBackground": "#222D32",
            "titleBar.inactiveBackground": "#222D32",
            "pickerGroup.border": "#222D32",
            "editorWidget.border": "#303B44",
            "editorCursor.foreground": "#D9D7CE",
            "editorBracketMatch.border": "#5CCFE6",
            "editorHoverWidget.border": "#222D32",
            "editorOverviewRuler.border": "#222D32",
            "editorSuggestWidget.border": "#222D32",
            "editorGroupHeader.tabsBorder": "#222D32",
            "peekView.border": "#222D32",
            "editorGroup.border": "#222D32",
            "tab.border": "#222D32",
            "button.background": "#36A3D9",
            "button.hoverBackground": "#5CCFE6",
            "extensionButton.prominentBackground": "#36A3D9",
            "extensionButton.prominentHoverBackground": "#5CCFE6",
            "button.foreground": "#FFFFFF",
            "tab.activeBorder": "#222D32",
            "tab.unfocusedActiveBorder": "#222D32",
            "panel.border": "#222D32",
            "statusBar.border": "#222D32",
            "statusBar.debuggingBackground": "#36A3D9",
            "statusBar.debuggingForeground": "#353535",
            "debugToolBar.background": "#303B44",
            "activityBar.border": "#222D32",
            "sideBar.border": "#222D32",
            "sideBar.foreground": "#D9D7CE",
            "titleBar.border": "#222D32",
            "editor.selectionBackground": "#3D4752",
            "editor.selectionHighlightBackground": "#5C6773",
            "editor.findMatchBackground": "#5C6773",
            "editor.findMatchHighlightBackground": "#5C6773",
            "debugExceptionWidget.border": "#222D32",
            "terminal.ansiBrightGreen": "#BAE67E"
        }
    },
    "workbench.startupEditor": "none",
    "window.zoomLevel": 0.8,
    "window.title": "${folderPath}${separator}${activeEditorMedium}",
    "window.menuBarVisibility": "toggle",
    "search.collapseResults": "alwaysCollapse",
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/vendor": true,
        "**/cache": true,
        "**/tmp": true
    },
    "explorer.openEditors.visible": 0,
    "explorer.confirmDragAndDrop": false,
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.cursorBlinking": "solid",
    "editor.lineHeight": 24,
    "editor.renderWhitespace": "all",
    "editor.tabCompletion": "on",
    "breadcrumbs.enabled": true,
    "files.trimTrailingWhitespace": true,
    "files.associations": {
        ".php_cs": "php",
        "Vagrantfile": "ruby"
    },
    "git.confirmSync": false,
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "diffEditor.renderSideBySide": false,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "gitlens.keymap": "none",
    "gitlens.defaultDateStyle": "absolute",
    "gitlens.defaultDateFormat": "YYYY-MM-DD",
    "gitlens.codeLens.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "php-cs-fixer.executablePath": "${extensionPath}/php-cs-fixer.phar",
    "php-cs-fixer.lastDownload": 1555880692578,
    "php-cs-fixer.onsave": true,
    "python.formatting.provider": "black",
    "todohighlight.isCaseSensitive": false,
    "todohighlight.include": [
        "**/*.php"
    ],
    "todohighlight.keywords": [
        "@todo", "@note", "todo:", "note:"
    ],
    "todohighlight.defaultStyle": {
        "color": "#FFFFFF",
        "backgroundColor": "#36A3D9",
        "isWholeLine": true
    },
    "todohighlight.exclude": [
        "**/node_modules/**",
        "**/bower_components/**",
        "**/dist/**",
        "**/build/**",
        "**/.vscode/**",
        "**/.github/**",
        "**/_output/**",
        "**/*.min.*",
        "**/*.map",
        "**/.next/**",
        "**/vendor/**",
        "**/pimcore/**"
    ],
    "todohighlight.isEnable": true
}